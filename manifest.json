{
  "manifest_version": 3,
  "name": "AUS Code Dictionary VNPZ v5.5",
  "version": "5.5",
  "author": "Удовеня Александр <alex31kv@yandex.ru>",
  "description": "Надстройка для AUS. Словарь кодов VNPZ.",

  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*AspenTech/AspenUnified/*",
        "*://*/*aspenTech/aspenUnified/*"
      ],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "*://*/*AspenTech/AspenUnified/*",
    "*://*/*aspenTech/aspenUnified/*"
  ]
}